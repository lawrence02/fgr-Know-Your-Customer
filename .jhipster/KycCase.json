  "applications": ["*"],
  "documentation": "KYC case tracking for each submission attempt",
  "fields": [
    {
      "fieldName": "kycRef",
      "fieldType": "String",
      "fieldValidateRules": ["required", "unique", "pattern"],
      "fieldValidateRulesPattern": "^FGR[0-9]{8}-[0-9]{3}$"
    },
    {
      "fieldName": "status",
      "fieldType": "KycStatus",
      "fieldValidateRules": ["required"],
      "fieldValues": "STARTED,IN_PROGRESS,AWAITING_DOCUMENTS,SUBMITTED,APPROVED,REJECTED,FAILED,EXPIRED,TIMEOUT"
    },
    {
      "fieldName": "channel",
      "fieldType": "ChannelType",
      "fieldValidateRules": ["required"],
      "fieldValues": "WHATSAPP,WEB,ANDROID,IOS,USSD"
    },
    {
      "fieldName": "startedAt",
      "fieldType": "Instant",
      "fieldValidateRules": ["required"]
    },
    {
      "fieldName": "lastActivityAt",
      "fieldType": "Instant",
      "fieldValidateRules": ["required"]
    },
    {
      "fieldName": "lastUpdatedAt",
      "fieldType": "Instant"
    },
    {
      "fieldName": "completedAt",
      "fieldType": "Instant"
    },
    {
      "fieldName": "expiresAt",
      "fieldType": "Instant"
    },
    {
      "fieldName": "validationErrors",
      "fieldType": "TextBlob"
    },
    {
      "fieldName": "internalNotes",
      "fieldType": "TextBlob"
    }
  ],
  "jpaMetamodelFiltering": true,
  "name": "KycCase",
  "pagination": "pagination",
  "relationships": [
