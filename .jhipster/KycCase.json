      "otherEntityRelationshipName": "kycCase",
      "relationshipName": "kycDocument",
      "relationshipSide": "left",
      "relationshipType": "one-to-many"
    },
    {
      "otherEntityName": "kycNotification",
      "otherEntityRelationshipName": "kycCase",
      "relationshipName": "kycNotification",
      "relationshipSide": "left",
