    <!--
        Added the constraints for entity KycCase.
    -->
    <changeSet id="20251214103013-2" author="jhipster">

        <addForeignKeyConstraint baseColumnNames="consent_id"
                                 baseTableName="kyc_case"
                                 constraintName="fk_kyc_case__consent_id"
